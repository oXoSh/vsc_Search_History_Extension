{
  "name": "search-hist",
  "displayName": "Search Hist",
  "description": "Show search history",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "searchHist.refreshSearchHistory",
        "title": "searchHist.refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "searchHist.copyNode",
        "title": "searchHist.copyNode",
        "icon": "$(clippy)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "searchHist",
          "name": "Search Hist",
          "icon": "resources/icon24.svg",
          "contextualTitle": "Search History"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "searchHist.refreshSearchHistory",
          "when": "view == searchHist",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.4.5"
  }
}